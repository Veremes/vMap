                ]
            },
            {
                "fields":[
                    {
                        "type":"text",
                        "name":"login",
                        "label":"FORM_LOGIN_USERS_DOMAIN",
                        "nb_cols":12
                    }
                ]
            },
            {
                "fields":[
                    {
                        "type":"password",
                        "name":"password",
                        "label":"FORM_PASSWORD_USERS_DOMAIN",
                        "nb_cols":12
                    }
                ]
            },
            {
                "fields":[
                    {
                        "type":"button",
