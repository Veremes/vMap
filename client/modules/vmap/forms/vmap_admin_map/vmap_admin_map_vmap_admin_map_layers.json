{
    "update":{
        "name":"vmap_map_map_layers_search_form",
        "title":"",
        "input_size":"xxs",
        "nb_cols":12,
        "rows":[
            {
                "fields":[
                    {
                        "type":"text",
                        "name":"name",
                        "label":"FORM_NAME_VMAP_MAP_MAP_LAYERS",
                        "nb_cols":4
                    },
                    {
                        "type":"select",
